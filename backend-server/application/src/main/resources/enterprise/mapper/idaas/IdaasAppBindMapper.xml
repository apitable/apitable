<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vikadata.api.enterprise.idaas.mapper.IdaasAppBindMapper">
    <select id="selectBySpaceId" resultType="com.vikadata.entity.IdaasAppBindEntity">
        SELECT *
        FROM ${tablePrefix}idaas_app_bind
        WHERE space_id = #{spaceId}
        AND is_deleted = 0
    </select>
</mapper>
