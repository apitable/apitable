<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vikadata.api.space.mapper.SpaceMenuMapper">

    <select id="selectByMenuCodes" resultType="com.vikadata.entity.SpaceMenuEntity">
        SELECT *
        FROM vika_space_menu vsm
        WHERE vsm.menu_code in
        <foreach collection="menuCodes" index="index" item="item" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </select>
</mapper>
