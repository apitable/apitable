{
  "name": "@apitable/core",
  "version": "0.18.0",
  "description": "datasheet core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc && tsc-alias",
    "build:publish": "rm -rf ./dist && tsc -p tsconfig.publish.json && tsc-alias -p tsconfig.publish.json && mv ./dist/index-publish.d.ts ./dist/index.d.ts",
    "start": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "test": "TZ=UTC-8 jest",
    "test:databus": "TZ=UTC-8 jest databus",
    "test:watch": "TZ=UTC-8 jest --watch",
    "test:cov": "TZ=UTC-8 jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "author": "APITable Ltd. <support@apitable.com>",
  "license": "AGPL-3.0",
  "engines": {
    "node": ">=8.x"
  },
  "dependencies": {
    "@apitable/i18n": "^1.0.4",
    "@types/lodash": "4.14.161",
    "@types/socket.io-client": "^1.4.36",
    "apphook": "1.0.6",
    "attr-accept": "^2.1.0",
    "axios": "0.21.2",
    "color": "^3.1.3",
    "dayjs": "^1.11.7",
    "fuse.js": "^6.4.1",
    "immer": "9.0.16",
    "joi": "^17.4.2",
    "json-schema": "^0.4.0",
    "lodash": "4.17.21",
    "lru-cache": "^5.1.1",
    "ot-json0": "^1.1.0",
    "phone": "^3.1.10",
    "re-reselect": "^4.0.0",
    "redux": "4.2.0",
    "redux-batched-actions": "0.5.0",
    "redux-thunk": "2.4.1",
    "reselect": "4.0.0",
    "semver": "^7.1.3",
    "socket.io-client": "2.2.0",
    "string.prototype.matchall": "^4.0.2",
    "urlcat": "^2.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/jest": "^29.2.1",
    "@types/lru-cache": "^5.1.1",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^14.14.34",
    "@types/qs": "^6.9.4",
    "@types/string.prototype.matchall": "^4.0.1",
    "fast-check": "^3.6.3",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "mockjs": "1.1.0",
    "ot-fuzzer": "^1.3.0",
    "ts-jest": "28.0.3",
    "ts-node": "^8.6.2",
    "ts-toolbelt": "^9.6.0",
    "tsc-alias": "^1.6.11",
    "typescript": "^4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".(spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "@apitable/i18n-lang"
    ]
  }
}
