{
  "name": "@apitable/room-server",
  "version": "1.8.0",
  "description": "nodejs room server",
  "author": "APITable Ltd. <support@apitable.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_DEBUG=bull ENABLE_SOCKET=true nest start",
    "start:dev": "pnpm start --watch",
    "start:debug": "pnpm start --debug --watch",
    "start:room-server:perf:flame": "clinic flame -- node dist/main",
    "start:room-server:perf:doctor": "clinic doctor -- node dist/main",
    "start:room-server:perf:heapprofiler": "clinic heapprofiler -- node dist/main",
    "start:room-server:perf:bubbleprof": "clinic bubbleprof -- node dist/main",
    "start:prod": "node dist/main",
    "start:room": "APPLICATION_NAME=ROOM_SERVER SERVER_PORT=3333 ENABLE_SOCKET=false nest start",
    "start:room:dev": "pnpm start:room --watch",
    "start:socket": " APPLICATION_NAME=SOCKET_SERVER SERVER_PORT=3001 ENABLE_SOCKET=true nest start",
    "start:socket:dev": "pnpm start:socket --watch",
    "lint": "eslint src",
    "test": "jest --logHeapUsage --verbose --silent --ci --updateSnapshot --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --colors --logHeapUsage --verbose --silent",
    "test:ut": "DOTENV_CONFIG_PATH=src/env/.env.dev-test jest --config ./jest-ut.config.json --colors --logHeapUsage --verbose --silent --ci --detectOpenHandles --forceExit",
    "test:ut:cov": "jest --config ./jest-ut.config.json --logHeapUsage --verbose --silent --ci --updateSnapshot --runInBand --detectOpenHandles --forceExit --testTimeout=60000 --coverage --coverageDirectory=coverage",
    "grpc:build": "tsproto --path src/proto --output src/proto/generated",
    "grpc:clean": "rimraf src/proto/generated",
    "copy:assets": "cp -r 'src/grpc/' 'dist/grpc/' && cp 'src/shared/helpers/list.lock.lua' 'dist/shared/helpers/list.lock.lua'"
  },
  "engines": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@apitable/core": "workspace:*",
    "@apitable/i18n-lang": "workspace:*",
