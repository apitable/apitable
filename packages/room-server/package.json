  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ],
    "moduleNameMapper": {
      "@apitable/databus-wasm-web": "<rootDir>/_skip_transpile_databus_wasm_web.ts"
    },
    "moduleDirectories": [
      "dist",
      "node_modules",
      "src"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
