{
  "name": "@apitable/room-server",
  "version": "0.18.0",
  "description": "nodejs room server",
  "author": "APITable Ltd. <support@apitable.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ENABLE_SOCKET=true nest start",
    "start:dev": "yarn start --watch",
    "start:debug": "yarn start --debug --watch",
    "start:perf:flame": "clinic flame -- node dist/main",
    "start:room:perf:flame": "clinic flame -- node dist/main",
    "start:prod": "node dist/main",
    "start:room": "APPLICATION_NAME=ROOM_SERVER SERVER_PORT=3333 ENABLE_SOCKET=false nest start",
    "start:room:dev": "yarn start:room --watch",
    "start:socket": " APPLICATION_NAME=SOCKET_SERVER SERVER_PORT=3001 ENABLE_SOCKET=true nest start",
    "start:socket:dev": "yarn start:socket --watch",
    "lint": "eslint -p tsconfig.json",
    "test": "jest --logHeapUsage --verbose --silent --ci --updateSnapshot --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --colors --logHeapUsage --verbose --silent",
    "test:ut": "DOTENV_CONFIG_PATH=src/env/.env.dev-test jest --config ./jest-ut.config.json --colors --logHeapUsage --verbose --silent --ci --detectOpenHandles --forceExit",
    "test:ut:cov": "jest --config ./jest-ut.config.json --logHeapUsage --verbose --silent --ci --updateSnapshot --runInBand --detectOpenHandles --forceExit --coverage --coverageDirectory=coverage",
    "grpc:build": "tsproto --path src/proto --output src/proto/generated",
    "grpc:clean": "rimraf src/proto/generated",
    "copy:assets": "cp -r 'src/grpc/' 'dist/grpc/' && cp 'src/shared/helpers/list.lock.lua' 'dist/shared/helpers/list.lock.lua'"
  },
  "engines": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@apitable/core": "*",
    "@apitable/i18n-lang": "*",
    "@apitable/nestjs-redis": "1.3.4",
    "@apitable/room-native-api": "*",
    "@aws-sdk/client-s3": "^3.47.0",
    "@golevelup/nestjs-rabbitmq": "^2.2.0",
    "@grpc/grpc-js": "~1.7.1",
    "@grpc/proto-loader": "^0.7.3",
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.1.2",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.1.2",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/microservices": "^8.1.2",
    "@nestjs/platform-fastify": "^8.1.2",
    "@nestjs/platform-socket.io": "^8.1.2",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.1.2",
    "@sentry/node": "^7.18.0",
    "@sentry/serverless": "^7.18.0",
    "@sentry/tracing": "^7.18.0",
    "@sentry/types": "^7.18.0",
    "@socket.io/redis-adapter": "^7.2.0",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "cli-color": "^2.0.0",
    "cls-hooked": "^4.2.2",
    "dayjs": "^1.11.7",
    "debug": "^4.3.1",
    "fast-safe-stringify": "^2.0.7",
    "fastify": "3.22.0",
    "fastify-helmet": "^5.0.0",
    "fastify-multipart": "^4.0.0",
    "fastify-swagger": "^4.3.3",
    "form-data": "^3.0.0",
    "getmac": "^5.20.0",
    "immer": "9.0.16",
    "ioredis": "^5.2.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "logform": "^2.2.0",
    "minio": "^7.0.26",
    "moment-timezone": "^0.5.35",
    "mysql2": "^2.3.2",
    "nest-winston": "^1.8.0",
    "nestjs-i18n": "^8.3.2",
    "nestjs-proto-gen-ts": "^1.0.19",
    "node-os-utils": "^1.3.5",
    "piscina": "^3.2.0",
    "pump": "^3.0.0",
    "qs": "^6.9.7",
    "rate-limiter-flexible": "^2.1.10",
    "redux": "4.2.0",
    "redux-batched-actions": "0.5.0",
    "redux-thunk": "2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "sha1": "^1.1.1",
    "socket.io": "^4.5.2",
    "ts-md5": "^1.3.1",
    "typeorm": "^0.2.38",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zipkin": "^0.22.0",
    "zipkin-context-cls": "^0.22.0",
    "zipkin-transport-http": "^0.22.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.2",
    "@types/cache-manager": "^3.4.2",
    "@types/cli-color": "^2.0.0",
    "@types/cls-hooked": "^4.3.0",
    "@types/hapi__joi": "^17.1.4",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "25.2.3",
    "@types/js-yaml": "^4.0.0",
    "@types/minio": "^7.0.11",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.6.2",
    "@types/node-os-utils": "^1.2.0",
    "@types/pump": "^1.1.1",
    "@types/sha1": "^1",
    "@types/socket.io": "^3.0.1",
    "@types/sprintf-js": "^1.1.2",
    "@types/supertest": "^2.0.10",
    "@types/zipkin-context-cls": "^0.11.3",
    "chai": "^4.2.0",
    "clinic": "^12.0.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "28.0.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ],
    "moduleDirectories": [
      "dist",
      "node_modules",
      "src"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
