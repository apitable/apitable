{
  "name": "@apitable/widget-sdk",
  "version": "0.17.0",
  "description": "vika widget sdk",
  "engines": {
    "node": ">=8.x"
  },
  "keywords": [
    "apitable",
    "widget",
    "sdk",
    "apitable widget sdk"
  ],
  "main": "./dist/index",
  "module": "./dist/index",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "APITable Ltd. <support@apitable.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "copy:static": "copyfiles -f src/static/*.png dist/static",
    "start": "rm -rf ./dist && yarn copy:static && concurrently \"tsc -w\" \"tsc-alias -w\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build": "rm -rf ./dist && yarn copy:static && tsc && tsc-alias",
    "lint": "eslint src/ examples/ --ext .tsx,.ts,.jsx,.js",
    "now-build": "npm run build",
    "docs": "node docs.js"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "@apitable/components": "*",
    "@apitable/core": "*",
    "ahooks": "^3.5.0",
    "attr-accept": "^2.1.0",
    "bowser": "^2.11.0",
    "emoji-mart": "^3.0.0",
    "fundebug-javascript": "^2.1.2",
    "loadjs": "^4.2.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash.without": "^4.4.0",
    "mime-types": "^2.1.29",
    "re-reselect": "^4.0.0",
    "react-redux": "8.0.4",
    "react-virtualized-auto-sizer": "1.0.2",
    "redux": "4.2.0",
    "redux-batched-actions": "0.5.0",
    "redux-thunk": "2.4.1",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "socket.io-client": "2.2.0",
    "store2": "^2.12.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.1",
    "@types/copyfiles": "^2",
    "@types/emoji-mart": "^3.0.8",
    "@types/jest": "^27.5.1",
    "@types/loadjs": "^4",
    "@types/localforage": "^0.0.34",
    "@types/mime-types": "^2.1.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "^7.1.9",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/socket.io-client": "^1.4.36",
    "@types/styled-components": "^5.1.26",
    "babel-jest": "^26.6.0",
    "concurrently": "^7.2.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.0",
    "jest": "28.1.0",
    "styled-components": "5.3.6",
    "ts-jest": "28.0.3",
    "tsc-alias": "^1.6.11",
    "typedoc": "0.21.9",
    "typedoc-plugin-markdown": "3.10.4",
    "typescript": "^4.8.2",
    "typescript-plugin-styled-components": "^2.0.0"
  }
}
